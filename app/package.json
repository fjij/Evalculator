{
  "name": "app",
  "version": "0.1.0",
  "main": "main.js",
  "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-packager-build-win32-x64": "electron-packager ./ \"evalculator\" --platform=\"win32\" --arch=\"x64\" --out=\"electron-builds\"",
    "electron-packager": "electron-packager ./ \"evalculator\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-packager": "^14.0.0"
  }
}
